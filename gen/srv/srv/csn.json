{
  "namespace": "mm",
  "definitions": {
    "mm.Role": {
      "kind": "type",
      "type": "cds.String",
      "length": 30,
      "enum": {
        "REQUESTER": {
          "val": "REQUESTER"
        },
        "TECH": {
          "val": "TECH"
        },
        "MANAGER": {
          "val": "MANAGER"
        }
      }
    },
    "mm.Status": {
      "kind": "type",
      "type": "cds.String",
      "length": 20,
      "enum": {
        "OPEN": {
          "val": "OPEN"
        },
        "ASSIGNED": {
          "val": "ASSIGNED"
        },
        "IN_PROGRESS": {
          "val": "IN_PROGRESS"
        },
        "DONE": {
          "val": "DONE"
        },
        "CLOSED": {
          "val": "CLOSED"
        }
      }
    },
    "mm.Users": {
      "kind": "entity",
      "includes": [
        "cuid"
      ],
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "name": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 100
        },
        "email": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 200
        },
        "role": {
          "@mandatory": true,
          "type": "mm.Role",
          "length": 30
        },
        "requests": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "mm.MaintenanceRequests",
          "on": [
            {
              "ref": [
                "requests",
                "requestedBy"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        },
        "assignments": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "mm.MaintenanceRequests",
          "on": [
            {
              "ref": [
                "assignments",
                "assignedTo"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        }
      }
    },
    "mm.Assets": {
      "kind": "entity",
      "includes": [
        "cuid"
      ],
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "code": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 50
        },
        "location": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 200
        },
        "info": {
          "type": "cds.String"
        },
        "requests": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "mm.MaintenanceRequests",
          "on": [
            {
              "ref": [
                "requests",
                "asset"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        }
      }
    },
    "mm.MaintenanceRequests": {
      "kind": "entity",
      "includes": [
        "cuid"
      ],
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "title": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 200
        },
        "description": {
          "type": "cds.String"
        },
        "status": {
          "@mandatory": true,
          "type": "mm.Status",
          "length": 20,
          "default": {
            "val": "OPEN"
          }
        },
        "priority": {
          "@mandatory": true,
          "type": "cds.Integer",
          "default": {
            "val": 1
          }
        },
        "createdAt": {
          "@cds.on.insert": {
            "=": "$now"
          },
          "type": "cds.Timestamp"
        },
        "updatedAt": {
          "@cds.on.insert": {
            "=": "$now"
          },
          "@cds.on.update": {
            "=": "$now"
          },
          "type": "cds.Timestamp"
        },
        "requestedBy": {
          "type": "cds.Association",
          "target": "mm.Users",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "assignedTo": {
          "type": "cds.Association",
          "target": "mm.Users",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "asset": {
          "type": "cds.Association",
          "target": "mm.Assets",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        }
      }
    },
    "MaintenanceService": {
      "@source": "srv/service.cds",
      "kind": "service",
      "@requires": "authenticated-user",
      "@path": "/maintenance"
    },
    "MaintenanceService.Users": {
      "kind": "entity",
      "@readonly": true,
      "@restrict": [
        {
          "grant": "READ",
          "to": "authenticated-user"
        }
      ],
      "projection": {
        "from": {
          "ref": [
            "mm.Users"
          ]
        },
        "columns": [
          "*",
          {
            "ref": [
              "requests"
            ],
            "cast": {
              "target": "MaintenanceService.MaintenanceRequests"
            }
          },
          {
            "ref": [
              "assignments"
            ],
            "cast": {
              "target": "MaintenanceService.MaintenanceRequests"
            }
          }
        ]
      },
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "name": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 100
        },
        "email": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 200
        },
        "role": {
          "@mandatory": true,
          "type": "mm.Role",
          "length": 30
        },
        "requests": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "MaintenanceService.MaintenanceRequests",
          "on": [
            {
              "ref": [
                "requests",
                "requestedBy"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        },
        "assignments": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "MaintenanceService.MaintenanceRequests",
          "on": [
            {
              "ref": [
                "assignments",
                "assignedTo"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        }
      }
    },
    "MaintenanceService.Assets": {
      "kind": "entity",
      "@readonly": true,
      "@restrict": [
        {
          "grant": "READ",
          "to": "authenticated-user"
        }
      ],
      "projection": {
        "from": {
          "ref": [
            "mm.Assets"
          ]
        },
        "columns": [
          "*",
          {
            "ref": [
              "requests"
            ],
            "cast": {
              "target": "MaintenanceService.MaintenanceRequests"
            }
          }
        ]
      },
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "code": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 50
        },
        "location": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 200
        },
        "info": {
          "type": "cds.String"
        },
        "requests": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "MaintenanceService.MaintenanceRequests",
          "on": [
            {
              "ref": [
                "requests",
                "asset"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        }
      }
    },
    "MaintenanceService.MaintenanceRequests": {
      "kind": "entity",
      "@restrict": [
        {
          "grant": [
            "READ"
          ],
          "to": "User"
        },
        {
          "grant": [
            "CREATE"
          ],
          "to": "User"
        },
        {
          "grant": [
            "UPDATE",
            "assign",
            "updateStatus"
          ],
          "to": "Tech"
        },
        {
          "grant": [
            "DELETE",
            "updatePriority"
          ],
          "to": "Admin"
        }
      ],
      "projection": {
        "from": {
          "ref": [
            "mm.MaintenanceRequests"
          ]
        },
        "columns": [
          "*",
          {
            "ref": [
              "requestedBy",
              "name"
            ],
            "as": "requesterName",
            "cast": {
              "type": "cds.String"
            }
          },
          {
            "ref": [
              "assignedTo",
              "name"
            ],
            "as": "technicianName",
            "cast": {
              "type": "cds.String"
            }
          },
          {
            "ref": [
              "asset",
              "code"
            ],
            "as": "assetCode",
            "cast": {
              "type": "cds.String"
            }
          }
        ]
      },
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "title": {
          "@mandatory": true,
          "type": "cds.String",
          "length": 200
        },
        "description": {
          "type": "cds.String"
        },
        "status": {
          "@mandatory": true,
          "type": "mm.Status",
          "length": 20,
          "default": {
            "val": "OPEN"
          }
        },
        "priority": {
          "@mandatory": true,
          "type": "cds.Integer",
          "default": {
            "val": 1
          }
        },
        "createdAt": {
          "@cds.on.insert": {
            "=": "$now"
          },
          "type": "cds.Timestamp"
        },
        "updatedAt": {
          "@cds.on.insert": {
            "=": "$now"
          },
          "@cds.on.update": {
            "=": "$now"
          },
          "type": "cds.Timestamp"
        },
        "requestedBy": {
          "type": "cds.Association",
          "target": "MaintenanceService.Users",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "assignedTo": {
          "type": "cds.Association",
          "target": "MaintenanceService.Users",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "asset": {
          "type": "cds.Association",
          "target": "MaintenanceService.Assets",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "requesterName": {
          "type": "cds.String"
        },
        "technicianName": {
          "type": "cds.String"
        },
        "assetCode": {
          "type": "cds.String"
        }
      },
      "actions": {
        "assign": {
          "kind": "action",
          "@restrict": [
            {
              "to": "Tech"
            }
          ],
          "params": {
            "technicianId": {
              "type": "cds.UUID"
            }
          }
        },
        "updateStatus": {
          "kind": "action",
          "@restrict": [
            {
              "to": "Tech"
            }
          ],
          "params": {
            "newStatus": {
              "type": "mm.Status",
              "length": 20
            }
          }
        },
        "updatePriority": {
          "kind": "action",
          "@restrict": [
            {
              "to": "Admin"
            }
          ],
          "params": {
            "newPriority": {
              "type": "cds.Integer"
            }
          }
        }
      }
    },
    "cds.outbox.Messages": {
      "kind": "entity",
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "timestamp": {
          "type": "cds.Timestamp"
        },
        "target": {
          "type": "cds.String"
        },
        "msg": {
          "type": "cds.LargeString"
        },
        "attempts": {
          "type": "cds.Integer",
          "default": {
            "val": 0
          }
        },
        "partition": {
          "type": "cds.Integer",
          "default": {
            "val": 0
          }
        },
        "lastError": {
          "type": "cds.LargeString"
        },
        "lastAttemptTimestamp": {
          "@cds.on.update": {
            "=": "$now"
          },
          "type": "cds.Timestamp"
        },
        "status": {
          "type": "cds.String",
          "length": 23
        }
      }
    },
    "Language": {
      "kind": "type",
      "@title": "{i18n>Language}",
      "@description": "{i18n>LanguageCode.Description}",
      "type": "cds.Association",
      "target": "sap.common.Languages",
      "keys": [
        {
          "ref": [
            "code"
          ]
        }
      ]
    },
    "Currency": {
      "kind": "type",
      "@title": "{i18n>Currency}",
      "@description": "{i18n>CurrencyCode.Description}",
      "type": "cds.Association",
      "target": "sap.common.Currencies",
      "keys": [
        {
          "ref": [
            "code"
          ]
        }
      ]
    },
    "Country": {
      "kind": "type",
      "@title": "{i18n>Country}",
      "@description": "{i18n>CountryCode.Description}",
      "type": "cds.Association",
      "target": "sap.common.Countries",
      "keys": [
        {
          "ref": [
            "code"
          ]
        }
      ]
    },
    "Timezone": {
      "kind": "type",
      "type": "cds.Association",
      "target": "sap.common.Timezones",
      "keys": [
        {
          "ref": [
            "code"
          ]
        }
      ]
    },
    "sap.common": {
      "kind": "context"
    },
    "sap.common.Locale": {
      "kind": "type",
      "@title": "{i18n>LanguageCode}",
      "type": "cds.String",
      "length": 14
    },
    "sap.common.Languages": {
      "kind": "entity",
      "@cds.autoexpose": true,
      "@cds.persistence.skip": "if-unused",
      "@UI.Identification": [
        {
          "Value": {
            "=": "name"
          }
        }
      ],
      "@cds.odata.valuelist": true,
      "includes": [
        "sap.common.CodeList"
      ],
      "elements": {
        "name": {
          "@title": "{i18n>Name}",
          "localized": true,
          "type": "cds.String",
          "length": 255
        },
        "descr": {
          "@title": "{i18n>Description}",
          "localized": true,
          "type": "cds.String",
          "length": 1000
        },
        "code": {
          "@Common.Text": {
            "=": "name"
          },
          "@title": "{i18n>LanguageCode}",
          "key": true,
          "type": "sap.common.Locale",
          "length": 14
        },
        "texts": {
          "type": "cds.Composition",
          "cardinality": {
            "max": "*"
          },
          "target": "sap.common.Languages.texts",
          "on": [
            {
              "ref": [
                "texts",
                "code"
              ]
            },
            "=",
            {
              "ref": [
                "code"
              ]
            }
          ]
        },
        "localized": {
          "type": "cds.Association",
          "target": "sap.common.Languages.texts",
          "on": [
            {
              "ref": [
                "localized",
                "code"
              ]
            },
            "=",
            {
              "ref": [
                "code"
              ]
            },
            "and",
            {
              "ref": [
                "localized",
                "locale"
              ]
            },
            "=",
            {
              "ref": [
                "$user",
                "locale"
              ]
            }
          ]
        }
      }
    },
    "sap.common.Countries": {
      "kind": "entity",
      "@cds.autoexpose": true,
